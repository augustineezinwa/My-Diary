{
  "name": "my-diary",
  "version": "1.0.0",
  "description": "API for building an application where I can jot my feelings",
  "engines": {
    "node": "14.11.0",
    "npm": "6.14.8"
  },
  "main": "build/server.js",
  "dependencies": {
    "@babel/runtime": "^7.14.8",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.6.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.7",
    "@babel/plugin-syntax-jsx": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/register": "^7.14.5",
    "babel-plugin-inline-json-import": "^0.3.2",
    "eslint": "^7.31.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.4",
    "nodemon": "^2.0.12"
  },
  "scripts": {
    "start:dev": "nodemon --exec babel-node API/server.js",
    "start": "node build/server.js",
    "test": "npm test",
    "build": "npm run clean && npm run build-assets && npm run db-migrate",
    "build-assets": "babel API --out-dir build",
    "clean": "rm -rf build",
    "db-migrate": "npx sequelize-cli db:migrate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/augustineezinwa/My-Diary.git"
